{
	"name": "ur",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev && tailwind:watch",
		"build": "vite build",
		"preview": "vite preview",
		"build:tailwind": "tailwind build -i src/app.css -o static/tailwind.css",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"tailwind:watch": "tailwind build -i src/app.css -o static/tailwind.css --watch",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/node": "^22.10.5",
		"autoprefixer": "^10.4.20",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.9",
		"tailwindcss-motion": "^1.0.1",
		"typescript": "^5.0.0",
		"vite": "^5.4.11"
	}
}
